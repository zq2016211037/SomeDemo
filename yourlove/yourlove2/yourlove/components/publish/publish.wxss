.showWrap{
  width:100%;
  height:100%;
  background: rgba(0,0,0,0.5);
  position: absolute;
  top:0;
  left: 0;
  z-index: 10;
}
.showWrap .close{
  width:100%;
  height:90rpx;
  background-color: #ffffff;
  display: flex;
  justify-content: center;
  padding-top: 20rpx;
  box-sizing: border-box;
  border-top:1rpx solid silver; 
}
.close image{
  width:60rpx;
  height:60rpx;
}
.publishNav{
  width:100%;
  height:150rpx;
  display: flex;
  justify-content: space-around;
  background-color: #ffffff;
}
.publishNav view{
  width:110rpx;
  height:120rpx;
  text-align:center;
}
.publishNav image{
  width:100rpx;
  height:100rpx;
}
.publishNav text{
  margin-top:-10rpx;
  font-size: 26rpx;
  display: block;
}
.publish{
  position: fixed;
  bottom:0;
  left:0;
  width:100%;
  padding-top:20rpx;
  color:#666666;
  background-color: #ffffff;
}